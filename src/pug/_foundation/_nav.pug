	]

nav#nav-main(class="navbar navbar-expand-lg navbar-dark")
	div.container
		a.navbar-brand(href="https://www.mangot5.com/Index")
			img(src="images/logo_mangot5.png" alt="happytuk")
		button(class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation")
			span(class="navbar-toggler-icon")
		
		div#navbarColor01(class="collapse navbar-collapse")
			ul(class="navbar-nav me-auto mb-2 mb-lg-0")
				each item in obj
					li(class="nav-item"): a(
						class="nav-item"
						href=`${item.link}` 
						id=`${item.id}`
						target="_blank"
						) #{item.text}
