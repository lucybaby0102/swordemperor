	var obj = [
		{ link: 'https://forum.gamer.com.tw/A.php?bsn=8294', text: '巴哈' },
		{ link: 'https://www.facebook.com/HOT.mangot5', text: 'FB' },
		{ link: 'https://www.mangot5.com/Index/Member/Register?gname=hot#step-1', text: '註冊', id : `${nav_regsiter_id}` },
		{ link: 'https://hot.mangot5.com/game/hot/download', text: '下載', id : `${nav_download_id}` },
		{ link: 'https://www.mangot5.com/Index/Member/Login?ref=/Index/Billing/MyCard/Payment', text: '儲值' },
		{ link: 'https://www.mangot5.com/Index/Member/Login', text: '登入', id : `${nav_login_id}`},
